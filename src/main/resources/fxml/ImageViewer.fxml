<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Pane fx:id="imagePane" style="-fx-background-color: #222521;">
               <children>
                  <ImageView fx:id="imageView" fitHeight="350.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </Pane>
            <AnchorPane fx:id="buttonPane" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="600.0" style="-fx-background-color: #181818;">
               <children>
                  <Button fx:id="cancelButton" layoutX="15.0" layoutY="10.0" mnemonicParsing="false" onAction="#onCancelButton" prefHeight="30.0" style="-fx-background-color: #404040;" text="Cancel" textFill="WHITE" />
                  <Button fx:id="oKButton" defaultButton="true" layoutX="86.0" layoutY="10.0" mnemonicParsing="false" onAction="#onOKButton" prefHeight="30.0" style="-fx-background-color: #404040;" text="OK" textFill="WHITE" AnchorPane.rightAnchor="15.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
